! do not scroll with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

URxvt.secondaryWheel: 1

URxvt.boldFont:
## URxvt.font: -nil-profont-medium-*-normal--12-*-*-*-*-*-*-*
URxvt.letterSpace: 0

##URxvt.perl-ext-common: default,matcher
##URxvt.url-launcher: /usr/bin/xdg-open
## URxvt.matcher.rend.0: Uline Bold fg5
URxvt.colorUL: #4682B4
URxvt*depth: 32
! vim: set ft=xdefaults:
URxvt.cursorBlink: 1
URxvt.cursorUnderline: 0



URxvt.intensityStyles: false
URxvt.scrollBar: false

! Ctrl Up and Down control font size
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger: C-Up
! copy paste
##URxvt.keysym.Shift-Control-V:   perl:clipboard:paste
##URxvt.keysym.Shift-Control-C:   perl:clipboard:copy

! disable ctrl-shift
URxvt.iso14755: false
URxvt.iso14755_52: false

! terminal padding
## URxvt.internalBorder: 15

Xft.autohint:   0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting:    true
Xft.antialias:  true
Xft.rgba:       rgb

! clickable links, clipboard support, mouse wheel support, and on the fly font resizing
##URxvt.perl-ext-common: default,matcher,clipboard,vtwheel,resize-font
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Bold fg6

